<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>etsTaskHandler</managed-bean-name>
  <managed-bean-class>com.etstaskHandler.EtsTaskHandler</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>menuHandler</managed-bean-name>
  <managed-bean-class>com.menuHandler.menuHandler</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>emReportHandler</managed-bean-name>
  <managed-bean-class>com.sample.EmReportHandler</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>logOnHandler</managed-bean-name>
  <managed-bean-class>com.bbh.ets.auth.LogOnHandler</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userCreationHandler</managed-bean-name>
  <managed-bean-class>com.bbh.ets.auth.UserCreationHandler</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>empHandler</managed-bean-name>
  <managed-bean-class>com.etstaskHandler.EmployeeHandler</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>projectHandler</managed-bean-name>
  <managed-bean-class>com.etstaskHandler.ProjectHandler</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <!-- Navigation -->
 <navigation-rule>
  <navigation-case>
   <from-outcome>log_off</from-outcome>
   <to-view-id>/logOn/logOn.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>ets_home</from-outcome>
   <to-view-id>/etsTask/taskList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>changePassword</from-outcome>
   <to-view-id>/logOn/changePassword.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>user_creation</from-outcome>
   <to-view-id>/user/userCreation.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>user_edition</from-outcome>
   <to-view-id>/user/userEdition.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>taskList</from-outcome>
   <to-view-id>/etsTask/taskList.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>taskListPerventRefesh</from-outcome>
   <to-view-id>/etsTask/taskList.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>addTask</from-outcome>
   <to-view-id>/etsTask/addTask.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>viewReports</from-outcome>
   <to-view-id>/report/employeeReport.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>editTask</from-outcome>
   <to-view-id>/etsTask/editTask.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/emp/addEmployee.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>user_add</from-outcome>
   <to-view-id>/user/userCreation.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>

 <navigation-rule>
  <navigation-case>
   <from-outcome>user_list</from-outcome>
   <to-view-id>/user/userList.xhtml</to-view-id>
    <redirect></redirect>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>employee_list</from-outcome>
   <to-view-id>/emp/showAllEmployee.xhtml</to-view-id>
   <redirect></redirect>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>cancel_emp_button</from-outcome>
   <to-view-id>/emp/showAllEmployee.xhtml</to-view-id>
    <redirect></redirect>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>add_employee</from-outcome>
   <to-view-id>/emp/addEmployee.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>emp_edition</from-outcome>
   <to-view-id>/emp/updateEmployee.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
  <navigation-rule>
  <navigation-case>
   <from-outcome>project</from-outcome>
   <to-view-id>/project/project.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
  <navigation-rule>
  <navigation-case>
   <from-outcome>projectHome</from-outcome>
   <to-view-id>/project/projectHome.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
</faces-config>
